autotrade.MouseButton1Click:Connect(function()
	local Frame2 = Frame_2

	if _G.autotrade == false then
		Frame2:TweenPosition(
			UDim2.new(0.747, 0, 0, 0),
			"Out",
			"Linear",
			0.3,
			false,
			nil
		)	
		wait(0.3)
		Frame2.Parent.ImageColor3 = Color3.fromRGB(85, 170, 0)
		_G.autotrade = true
	end

end)

autotrade.MouseButton1Click:Connect(function()
	local Frame2 = Frame_2
	if _G.autotrade == true then
		Frame2:TweenPosition(
			UDim2.new(-0.024, 0, 0, 0),
			"Out",
			"Linear",
			0.3,
			false,
			nil
		)	
		wait(0.3)
		Frame2.Parent.ImageColor3 = Color3.fromRGB(170, 0, 0)
		_G.autotrade = false
	end
end)

console.MouseButton1Click:Connect(function()
	local Frame2 = Frame_3

	if _G.consolelog == false then
		Frame2:TweenPosition(
			UDim2.new(0.747, 0, 0, 0),
			"Out",
			"Linear",
			0.3,
			false,
			nil
		)	
		wait(0.3)
		Frame2.Parent.ImageColor3 = Color3.fromRGB(85, 170, 0)
		_G.consolelog = true
		rconsolename("Clicker Simulator Auto Trade Logs")
	end

end)

