-- Main Script

  local handler = workspace:FindFirstChild("Handler")

  if not handler then
      local Folder = Instance.new("Folder")
      Folder.Parent = workspace
      Folder.Name = "Handler"
  end

  local players = {}
  local blocklist = {}
  
  for i,v in pairs(game.Players:GetPlayers())do
      if not table.find(players, v.Name) and v.Name ~= game.Players.LocalPlayer.Name then
          table.insert(players, v.Name)
      end
  end
  
  game.Players.PlayerAdded:Connect(function(player)
      if not table.find(players, player.Name) then
          table.insert(players, player.Name)
      end
  end)
  
  game.Players.PlayerRemoving:Connect(function(player)
      if not table.find(players, player.Name) then
          table.insert(players, player.Name)
      end
  end)
  
  local function sendTrade(target)
	local args = {
		[1] = target
	}

	game:GetService("ReplicatedStorage").Events.Client.trading.sendTradeRequest:FireServer(unpack(args))

  end
  
  local function tpserver()
      local ts = game:GetService("TeleportService")
      if _G.allowbottp == true then
          _G.status = "Teleporting"
          ts:Teleport(game.PlaceId, game.Players.LocalPlayer)
          print("zaaa")
      end
  end
  
  local function getTarget()
      if table.maxn(blocklist) >= 4 then
          tpserver()
      end
  
      for i,v in pairs(game.Players:GetPlayers())do
          if not table.find(blocklist,v.DisplayName) then
            if v.Name ~= game.Players.LocalPlayer.Name then
				sendTrade(v.Name)
			end
          end
      end
  end
  
  game:GetService("RunService").RenderStepped:Connect(function()
      wait(5)
      if _G.autotrade == true and _G.trading == false then
          getTarget()
      end
      wait(55)
      if _G.tradecount <= _G.tpval and _G.trading == false then
          tpserver()
      elseif _G.tradecount <= _G.tpval and _G.trading == true then
          wait(10)
          if _G.tradecount <= _G.tpval then
              tpserver()
          end
      elseif _G.tradecount > _G.tpval then
          _G.tpval = _G.tpval + 2
      end
  end)

  local function unabbreviate(number)
	if string.find(number,"K") then
		if string.sub(number,2,2) == "K" and string.len(number) == 2 then
			number = string.gsub(number,"K","000")
		end
		if string.sub(number,2,2) == "." then
			if string.sub(number,4,4) == "K" then
				number = string.sub(number,1,1)..string.sub(number,3,3).."00"
			end
		end
		if string.sub(number,2,2) == "." then
			if string.sub(number,5,5) == "K" then
				number = string.sub(number,1,1)..string.sub(number,3,3)..string.sub(number,4,4).."0"
			end
		end
		if string.sub(number,3,3) == "K" and string.len(number) == 3 then
			number = string.gsub(number,"K","000")
		end
		if string.sub(number,3,3) == "." then
			if string.sub(number,5,5) == "K" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,4,4).."00"
			end
		end
		if string.sub(number,3,3) == "." then
			if string.sub(number,6,6) == "K" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,4,4)..string.sub(number,5,5).."0"
			end
		end
		if string.sub(number,4,4) == "K" and string.len(number) == 4 then
			number = string.gsub(number,"K","000")
		end
		if string.sub(number,4,4) == "." then
			if string.sub(number,6,6) == "K" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,3,3)..string.sub(number,5,5).."00"
			end
		end
		if string.sub(number,4,4) == "." then
			if string.sub(number,7,7) == "K" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,3,3)..string.sub(number,5,5)..string.sub(number,6,6).."0"
			end
		end
	end
	if string.find(number,"M") then
		if string.sub(number,2,2) == "M" and string.len(number) == 2 then
			number = string.gsub(number,"M","000000")
		end

		if string.sub(number,2,2) == "." then
			if string.sub(number,4,4) == "M" then
				number = string.sub(number,1,1)..string.sub(number,3,3).."00000"
			end
		end
		if string.sub(number,2,2) == "." then
			if string.sub(number,5,5) == "M" then
				number = string.sub(number,1,1)..string.sub(number,3,3)..string.sub(number,4,4).."0000"
			end
		end
		if string.sub(number,3,3) == "M" and string.len(number) == 3 then
			number = string.gsub(number,"M","000000")
		end
		if string.sub(number,3,3) == "." then
			if string.sub(number,5,5) == "M" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,4,4).."00000"
			end
		end
		if string.sub(number,3,3) == "." then
			if string.sub(number,6,6) == "M" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,4,4)..string.sub(number,5,5).."0000"
			end
		end
		if string.sub(number,4,4) == "M" and string.len(number) == 4 then
			number = string.gsub(number,"M","000000")
		end
		if string.sub(number,4,4) == "." then
			if string.sub(number,6,6) == "M" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,3,3)..string.sub(number,5,5).."00000"
			end
		end
		if string.sub(number,4,4) == "." then
			if string.sub(number,7,7) == "M" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,3,3)..string.sub(number,5,5)..string.sub(number,6,6).."0000"
			end
		end
	end
	if string.find(number,"B") then
		if string.sub(number,2,2) == "B" and string.len(number) == 2 then
			number = string.gsub(number,"B","000000000")
		end
		if string.sub(number,2,2) == "." then
			if string.sub(number,4,4) == "B" then
				number = string.sub(number,1,1)..string.sub(number,3,3).."00000000"
			end
		end
		if string.sub(number,2,2) == "." then
			if string.sub(number,5,5) == "B" then
				number = string.sub(number,1,1)..string.sub(number,3,3)..string.sub(number,4,4).."0000000"
			end
		end
		if string.sub(number,3,3) == "B" and string.len(number) == 3 then
			number = string.gsub(number,"B","000000000")
		end
		if string.sub(number,3,3) == "." then
			if string.sub(number,5,5) == "B" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,4,4).."00000000"
			end
		end
		if string.sub(number,3,3) == "." then
			if string.sub(number,6,6) == "B" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,4,4)..string.sub(number,5,5).."0000000"
			end
		end
		if string.sub(number,4,4) == "B" and string.len(number) == 4 then
			number = string.gsub(number,"B","000000000")
		end
		if string.sub(number,4,4) == "." then
			if string.sub(number,6,6) == "B" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,3,3)..string.sub(number,5,5).."00000000"
			end
		end
		if string.sub(number,4,4) == "." then
			if string.sub(number,7,7) == "B" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,3,3)..string.sub(number,5,5)..string.sub(number,6,6).."0000000"
			end
		end
	end
	if string.find(number,"T") then
		if string.sub(number,2,2) == "T" and string.len(number) == 2 then
			number = string.gsub(number,"T","000000000000")
		end
		if string.sub(number,2,2) == "." then
			if string.sub(number,4,4) == "T" then
				number = string.sub(number,1,1)..string.sub(number,3,3).."00000000000"
			end
		end
		if string.sub(number,2,2) == "." then
			if string.sub(number,5,5) == "T" then
				number = string.sub(number,1,1)..string.sub(number,3,3)..string.sub(number,4,4).."0000000000"
			end
		end
		if string.sub(number,3,3) == "T" and string.len(number) == 3 then
			number = string.gsub(number,"T","000000000000")
		end
		if string.sub(number,3,3) == "." then
			if string.sub(number,5,5) == "T" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,4,4).."00000000000"
			end
		end
		if string.sub(number,3,3) == "." then
			if string.sub(number,6,6) == "T" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,4,4)..string.sub(number,5,5).."0000000000"
			end
		end
		if string.sub(number,4,4) == "T" and string.len(number) == 4 then
			number = string.gsub(number,"T","000000000000")
		end
		if string.sub(number,4,4) == "." then
			if string.sub(number,6,6) == "T" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,3,3)..string.sub(number,5,5).."00000000000"
			end
		end
		if string.sub(number,4,4) == "." then
			if string.sub(number,7,7) == "T" then
				number = string.sub(number,1,1)..string.sub(number,2,2)..string.sub(number,3,3)..string.sub(number,5,5)..string.sub(number,6,6).."0000000000"
			end
		end
		
	end
	return number
end
  
  local function ready()
    game:GetService("ReplicatedStorage").Events.Client.trading.readyToggle:FireServer()
	_G.status = "Ready!"
	wait(0.1)
	_G.status.Text = "CanEge's Clicker Simulator Auto Trade Bot"
end

local function getOpponentsValue()
    local value = "0"
    for _,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.otherBG.Scroll:GetChildren())do
        if v:IsA("ImageButton") and v.Visible == true then
            value = value + unabbreviate(v:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text)
			print(value)
        end
    end
    return value
end

local function getLocalValue()
    local value = "0"
    for _,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
        if v:IsA("ImageButton") and v:FindFirstChild("eq") then
            value = value + unabbreviate(v:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text)
			print(value)
        end
    end
    return value
end

local function sendWHMessage(info)
	local msg = {
		 
		["embeds"] = {{
			["title"] = "**Clicker Simulator Auto Trade Bot**:",
			["description"] = tostring(info),
			["color"] = tonumber(0x6a0dad),
			
			["fields"] = {
				{
					["name"] = "Values:",
					["value"] = getLocalValue().."  /  "..getOpponentsValue(),
					["inline"] = false
	 },
				{
					["name"] = "Opponent's Name",
					["value"] = game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.titleB.Text,--game:GetService("Players").LocalPlayer.PlayerGui.Trading.Frame.Trade.Player.Title.Text,
					["inline"] = false
	 },
	},
	}
	}
	}
		print("zaaa")
		HttpRequest({Url=_G.webhook.Text, Body=game:GetService("HttpService"):JSONEncode(msg), Method="POST", Headers={["content-type"] = "application/json"}})
	end
	 
	

local function removePet(petname,petfolder)
    print("removing pet(s)")
    local args = {
        [1] = petname
    }

	for _,v2 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
        if v2:FindFirstChild("Background") then
            if v2.petID.Value == petname and v2:FindFirstChild("eq") then
                if workspace:FindFirstChild("Handler"):FindFirstChild(petfolder.petID.Value) then
                    if workspace:FindFirstChild("Handler"):FindFirstChild(petfolder.petID.Value):FindFirstChild(petname) then
                        workspace:FindFirstChild("Handler"):FindFirstChild(petfolder.petID.Value):FindFirstChild(petname):Destroy()
                        if v2:FindFirstChild("eq") then
                            v2:FindFirstChild("eq"):Destroy()
                        end
                        game:GetService("ReplicatedStorage").Events.Client.trading.updateTradePets:FireServer(unpack(args))
						_G.status.Text = "Removing pet(s)..."
						wait(0.3)
						_G.status.Text = getLocalValue().."  /  "..getOpponentsValue()
						wait(0.2)
						_G.status.Text = "CanEge's Clicker Simulator Auto Trade Bot"
                    end
                end
            end
        end
    end
end

local function checkStats()

	if ""..getLocalValue() <= ""..getOpponentsValue() then
		return true
	else
		return false
	end
end

local function addPet(petname, petfolder)
    print("adding pet(s)")

    local args = {
        [1] = petname
    }
    
    for _,v2 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
        if v2:IsA("ImageButton") then
            if v2.petID.Value == petname and not v2:FindFirstChild("eq") and unabbreviate(v2:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text) + getLocalValue() <= getOpponentsValue() and not v2:FindFirstChild("eq") and _G.autotrade == true and _G.freepetmode == false and _G.usemaxval == false then
                if workspace:FindFirstChild("Handler"):FindFirstChild(petfolder.petID.Value) then
					local Folder = Instance.new("Folder")
					Folder.Name = v2.petID.Value
					Folder.Parent = workspace:FindFirstChild("Handler"):FindFirstChild(petfolder.petID.Value)
					local Folder2 = Instance.new("Folder")
					Folder2.Name = "eq"
					Folder2.Parent = v2
					game:GetService("ReplicatedStorage").Events.Client.trading.updateTradePets:FireServer(unpack(args))
					_G.status.Text = "Adding pet(s)"
					wait(0.2)
					_G.status.Text = getLocalValue().."  /  "..getOpponentsValue()
					wait(0.2)
					_G.status.Text = "CanEge's Clicker Simulator Auto Trade Bot"
                end
			elseif  v2.petID.Value == petname and not v2:FindFirstChild("eq") and unabbreviate(v2:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text) + getLocalValue() <= getOpponentsValue() and not v2:FindFirstChild("eq") and _G.autotrade == true and _G.freepetmode == false and _G.usemaxval == true and unabbreviate(v2:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text) + getLocalValue() <= tonumber(unabbreviate(_G.maxval.Text)) then
				if workspace:FindFirstChild("Handler"):FindFirstChild(petfolder.petID.Value) then
					local Folder = Instance.new("Folder")
					Folder.Name = v2.petID.Value
					Folder.Parent = workspace:FindFirstChild("Handler"):FindFirstChild(petfolder.petID.Value)
					local Folder2 = Instance.new("Folder")
					Folder2.Name = "eq"
					Folder2.Parent = v2
					game:GetService("ReplicatedStorage").Events.Client.trading.updateTradePets:FireServer(unpack(args))
					_G.status.Text = "Adding pet(s)"
					wait(0.2)
					_G.status.Text = getLocalValue().."  /  "..getOpponentsValue()
					wait(0.2)
					_G.status.Text = "CanEge's Clicker Simulator Auto Trade Bot"
                end
			end
        end
	end
end

game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.otherBG.Scroll.ChildAdded:Connect(function(item)
	print("child added to scroll")
	if not workspace:FindFirstChild("Handler"):FindFirstChild(item.petID.Value) then
		local Folder = Instance.new("Folder")
		Folder.Name = item.petID.Value
		Folder.Parent = workspace:FindFirstChild("Handler")
	end

        for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
			--print(v.petID.Value)
            if v:IsA("ImageButton") and v:FindFirstChild("Background"):FindFirstChild("selected").Visible == false and unabbreviate(v:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text) + getLocalValue() <= getOpponentsValue() and not v:FindFirstChild("eq") and _G.freepetmode == false then
				wait(0.1)
				print("yesssir")
				addPet(v.petID.Value,item)
            end
        end

		if _G.freepetmode then
			ready()
		end

	wait()

	if checkStats() == true then
		ready()
	end

end)

game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.otherBG.Scroll.ChildRemoved:Connect(function(item)
    print("childremoved scroll")
	for _,v2 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
		if v2:IsA("ImageButton") then
			if _G.freepetmode == false then
				print("yesss finally")
				removePet(v2.petID.Value,item)
			else
				ready()
			end
        end

		--[[if workspace:FindFirstChild("Handler"):FindFirstChild(item.petID.Value) then
			workspace:FindFirstChild("Handler"):FindFirstChild(item.petID.Value):Destroy()
		end--]]
	end

    wait()

	if checkStats() == true then
		ready()
	end
end)

local function a()
    while wait() do
		for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.otherBG.Scroll:GetChildren())do
			if v:IsA("ImageButton") and v.Visible == false then
				v:Destroy()
			end
		end
		for i2,v2 in pairs(game.Workspace:FindFirstChild("Handler"):GetChildren())do
		    if _G.trading == false and v2 then
		        v2:Destroy()
		    end
		end
		
		for _,v3 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
			if v3:IsA("ImageButton") and v3:FindFirstChild("eq") and _G.trading == false then
				v3:FindFirstChild("eq"):Destroy()
			end
		end
        
    end
end

coroutine.wrap(a)()
game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground:GetPropertyChangedSignal("Visible"):Connect(function()
	if game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Visible == true then
		_G.trading = true
		_G.tradecount = _G.tradecount + 1
	else
		_G.trading = false
	end
end)

print("YES")
if game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Visible == true then
	_G.trading = true
end

game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeStatusBackground:GetPropertyChangedSignal("Visible"):Connect(function()
    local t = game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeStatusBackground.Background.description.Text
    local n = game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.titleB.Text

    if game:GetService("Players").LocalPlayer.PlayerGui.framesUI:FindFirstChild("tradeStatusBackground").Visible == true then
    wait()
    if t == "Trade was cancelled!" then
        if _G.webhooklog == true then
            sendWHMessage("Trade was cancelled!")
        end
        if _G.consolelog == true then
            rconsoleinfo("Trade was cancelled! "..n)
        end
        print("finally")
    end
    
    if t == "The trade has ended. No pets were traded." then
        if _G.webhooklog == true then
            sendWHMessage("The trade has ended. No pets were traded.")
        end
        if _G.consolelog == true then
            rconsoleinfo("The trade has ended No pets were traded. "..n)
        end
        print("finally2")
    end
    
    if t == "Trade successfully completed!" then
        if _G.webhooklog == true then
            sendWHMessage("Trade successfully completed")
        end
        if _G.consolelog == true then
            rconsoleinfo("Trade successfully completed "..n)
        end

        print("finally3")
    end

    print("yessirr")
    end
end)

game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground:FindFirstChild("Background").readyB:GetPropertyChangedSignal("Visible"):Connect(function()
    if game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground:FindFirstChild("Background").readyB.Visible == true and _G.autotrade == true then
        if checkStats() == true then
            ready()
        end
    end
end)

