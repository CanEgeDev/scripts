	-- Scripts:
	
	local function CLXF_fake_script() -- Button.LocalScript 
		local script = Instance.new('LocalScript', Button)
	
		local t = script.Parent.TextLabel
		script.Parent.MouseButton1Click:Connect(function()
			local TweenService = game:GetService("TweenService")
			local Frame2 = script.Parent.Parent
		
			if t.Text == "+" then
				Frame2:TweenSize(
					UDim2.new(0, 222, 0, 232),
					"Out",
					"Linear",
					0.3,
					false,
					nil
				)	
				wait(0.3)
				t.Text = "-"
				for i,v in pairs(Frame2:GetChildren())do
					if v.Name ~= "yo" and not v:IsA("LocalScript") and v ~= script.Parent and v.Name ~= "ScrollingFrame" then
						v.Visible = true
					end
				end
			end
			
		end)
		
		script.Parent.MouseButton1Click:Connect(function()
			local TweenService = game:GetService("TweenService")
			local Frame2 = script.Parent.Parent
			if t.Text == "-" then
				Frame2:TweenSize(
					UDim2.new(0, 222, 0, 24),
					"Out",
					"Linear",
					0.3,
					false,
					nil
				)	
				for i,v in pairs(Frame2:GetChildren())do
					if v.Name ~= "yo" and not v:IsA("LocalScript") and v ~= script.Parent then
						v.Visible = false
					end
				end
				wait(0.3)
				t.Text = "+"
			end
			
		end)
		
	end
	coroutine.wrap(CLXF_fake_script)()
	local function DLZOPIV_fake_script() -- selectpet.LocalScript 
		local script = Instance.new('LocalScript', selectpet)
	
		local lastframe = nil
		local selected = nil
		local ListLayout = script.Parent.Parent.ScrollingFrame.ListLayout
		local guid = {}
		
		local function createButton(v)
			if v.Name then
				local f = Instance.new("TextButton")
				f.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
				f.Size = UDim2.new(0, 191, 0, 29)
				f.Font = Enum.Font.Roboto
				f.TextColor3 = Color3.fromRGB(255, 255, 255)
				f.TextSize = 14.000
				f.TextStrokeColor3 = Color3.fromRGB(20, 20, 20)
				f.Text = v.Name
				f.AutomaticSize = "Y"
				if lastframe == nil then
					f.Position = UDim2.new(0, 0, 0,0)
				else
					f.Position = UDim2.new(lastframe.Position.X, lastframe.Position.Y + UDim.new(0.062,0))
				end
		
				f.MouseButton1Click:Connect(function()
					selected = v
					script.Parent.Text = v.Name.." ["..v:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text.."]".."Selected"
					script.Parent.Parent.ScrollingFrame.Visible = false
					lastframe = nil
					for _,v in pairs(script.Parent.Parent.ScrollingFrame:GetChildren())do
						if v:IsA("TextButton") then
							v:Destroy()
						end
					end
					print(selected)
				end)
		
				f.Parent = script.Parent.Parent.ScrollingFrame
				lastframe = f
			end
		end
		
		script.Parent.MouseButton1Click:Connect(function()
			if script.Parent.Parent.ScrollingFrame.Visible == false then
				script.Parent.Parent.ScrollingFrame.Visible = true
				lastframe = nil
				for _,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.petsBackground.Background:FindFirstChild("background").Scroll.ScrollingFrame:GetChildren())do
					if v:IsA("ImageButton") then
						createButton(v)
					end
				end
		
			else
				script.Parent.Parent.ScrollingFrame.Visible = false
				lastframe = nil
				for _,v in pairs(script.Parent.Parent.ScrollingFrame:GetChildren())do
					if v:IsA("TextButton") then
						v:Destroy()
					end
				end
			end
		
		end)
		
		local p = {}
		
		script.Parent.Parent.confirm.MouseButton1Click:Connect(function()
		
			local newval = script.Parent.Parent.petval.Text
		
			for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.petsBackground.Background:FindFirstChild("background").Scroll.ScrollingFrame:GetChildren())do
				if v:IsA("ImageButton") then
					if v.Name == selected.Name then
						v:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text = newval
					end
				end
			end
		
			for i2,v2 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
				if v2:IsA("ImageButton") and v2.Name == selected.Name then
					v2:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text = newval
				end
			end
			
			p[selected.Name] = newval
			
			for i2,v2 in pairs(game:GetService("Players").LocalPlayer:FindFirstChild("petOwned"):GetChildren())do
				if v2.Name == selected.petID.Value.."" then
					guid[selected.Name] = v2:FindFirstChild("GUID").Value
				end
			end
		
		end)
		
		for _,v2 in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
			if v2:IsA("ImageButton") and p[v2.Name] and p[v2.Name] ~= nil then
				v2:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text = p[v2.Name]
			end
		end
		
		
		while wait()do
			for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.tradeActiveBackground.Background.youBG.Scroll:GetChildren())do
				if p[v.Name] and p[v.Name] ~= nil then
					for i2,v2 in pairs(game:GetService("Players").LocalPlayer:FindFirstChild("petOwned"):GetChildren())do
						if v2.Name == v:FindFirstChild("petID").Value.."" then
							if guid[v.Name] and guid[v.Name] == v2:FindFirstChild("GUID").Value then
								v:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text = p[v.Name]
							end
						end
					end
				end
			end
		
			for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.framesUI.petsBackground.Background:FindFirstChild("background").Scroll.ScrollingFrame:GetChildren())do
				if v:IsA("ImageButton") and v.Visible == true then
					if p[v.Name] and p[v.Name] ~= nil then
						for i2,v2 in pairs(game:GetService("Players").LocalPlayer:FindFirstChild("petOwned"):GetChildren())do
							if v2.Name == v:FindFirstChild("petID").Value.."" then
								if guid[v.Name] and guid[v.Name] == v2:FindFirstChild("GUID").Value then
									v:FindFirstChild("Background"):FindFirstChild("Background").multiplier.Text = p[v.Name]
								end
							end
						end
					end
				end
			end
		
		end
	end
		coroutine.wrap(DLZOPIV_fake_script)()
	end
	
	
